{
    "editor.tabSize": 2,
    "files.insertFinalNewline": true,
    "python.analysis.extraPaths": [
        "/usr/local/lib/python3.8/dist-packages",
        "/usr/lib/python3/dist-packages",
        "/usr/lib/python3.8/dist-packages",
        "/home/${env:USERNAME}/.local/lib/python3.8/site-packages"
    ],
    "cSpell.userWords": [],
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "diffEditor.ignoreTrimWhitespace": false,
    "vim.useSystemClipboard": true,
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "u"
            ],
            "commands": [
                "undo"
            ]
        },
        {
            "before": [
                "<C-r>"
            ],
            "commands": [
                "redo"
            ]
        },
    ],
    "window.zoomLevel": 2,
    "python.defaultInterpreterPath": "/bin/python3",
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnType": true,
    },
    "editor.formatOnSave": true,
    "deepl.apiKey": "fa603094-5289-8e95-9b8c-63f2126b4a47:fx",
    "cmake.configureOnOpen": true,
    "hediet.vscode-drawio.resizeImages": null,
    "java.format.settings.url": "https://raw.githubusercontent.com/google/styleguide/gh-pages/eclipse-java-google-style.xml",
    "java.format.settings.profile": "GoogleStyle",
    "java.checkstyle.configuration": "${workspaceFolder}/google_checks.xml",
    "java.checkstyle.version": "10.12.2",
    "[java]": {
        "editor.defaultFormatter": "redhat.java"
    }
}
